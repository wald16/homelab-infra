# ================================
# Homelab Infra - Example .env
# ================================

# --- ROMM / MariaDB ---
DB_ROOT=changeme                 # Root password for MariaDB
DB_PASSWD=changeme               # Database password for ROMM
ROMM_AUTH_SECRET_KEY=changeme    # Secret key for ROMM authentication

# --- n8n Automations ---
N8N_USR=changeme                 # Basic auth user for n8n
N8N_PSWD=changeme                # Basic auth password for n8n
n8n_access_token=changeme        # n8n personal access token
N8N_ENCRYPT_KEY=changeme         # Encryption key for n8n
CLOUDFLARE_TUNNEL_TOKEN_N8N=changeme   # Cloudflare tunnel token for n8n

# --- Entradas App ---
CLOUDFLARE_TUNNEL_TOKEN_ENTRADAS=changeme   # Cloudflare tunnel token for Entradas app
DB_ENTRADAS_PASS=changeme                   # Postgres password for Entradas app

# --- IG Bot ---
CLOUDFLARE_TUNNEL_TOKEN_NGINX_IG_BOT=changeme   # Cloudflare tunnel token for Instagram bot
